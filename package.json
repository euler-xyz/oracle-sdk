{
  "name": "@objectivelabs/oracle-sdk",
  "version": "1.1.4",
  "description": "JavaScript utilities for Euler price oracles",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "clean": "rimraf dist",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "tsup src/test.ts --format cjs,esm && node dist/test.js",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "euler",
    "oracle",
    "ethereum",
    "defi",
    "price-feed"
  ],
  "author": "Objective Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/objectivedefi/oracle-sdk.git"
  },
  "devDependencies": {
    "@swc-node/register": "^1.10.9",
    "@swc/core": "^1.10.12",
    "@types/node": "^20.13.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.5.3",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "eslint-import-resolver-typescript": "^3.6.1"
  },
  "dependencies": {
    "viem": "^2.23.3"
  }
}
